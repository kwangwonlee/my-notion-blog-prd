{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": ["@playwright/mcp@latest"],
      "env": {},
      "name": "playwright"
    },
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp",
      "name": "context7"
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "name": "sequential-thinking"
    },
    "shadcn": {
      "command": "npx",
      "args": ["shadcn@latest", "mcp"],
      "name": "shaden"
    }
  }
}